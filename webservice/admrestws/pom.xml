<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <properties>
        <endorsed.dir>${project.build.directory}/endorsed</endorsed.dir>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <admrestws.version>1.07.04</admrestws.version>
        <login-admin.version>1.00.01</login-admin.version>
        <login-common.version>1.00.01</login-common.version>
        <password-service.version>1.00.01</password-service.version>
        <department-service.version>1.02.01-SNAPSHOT</department-service.version>
        <journal-service.version>1.02.01-SNAPSHOT</journal-service.version>
        <menu-service.version>1.00.00-SNAPSHOT</menu-service.version>
        <service-caller.version>1.01.03</service-caller.version>
        <service-utils.version>1.01.01</service-utils.version>
        <config-utils.version>1.00.00</config-utils.version>
        <date-utils.version>1.00.00</date-utils.version>
        <string-utils.version>1.00.02</string-utils.version>
        <external-caller.version>1.00.01</external-caller.version>
        <xml-utils.version>1.00.00</xml-utils.version>
        <service-loader-utils.version>1.00.00</service-loader-utils.version>
        <password-utils.version>1.00.01</password-utils.version>
        <log4j2.version>2.6.2</log4j2.version>
    </properties>

    <groupId>com.bivgroup.flextera.insurance.bivsberfront.webservice</groupId>
    <artifactId>admrestws</artifactId>
    <version>${admrestws.version}</version>
    <packaging>war</packaging>
    <name>admrestws</name>

    <dependencies>
        <dependency>
            <groupId>com.bivgroup</groupId>
            <artifactId>service-caller</artifactId>
            <version>${service-caller.version}</version>
        </dependency>
        <dependency>
            <groupId>com.bivgroup.flextera.insurance.bivsberfront.utils</groupId>
            <artifactId>service-utils</artifactId>
            <version>${service-utils.version}</version>
        </dependency>
        <dependency>
            <groupId>com.bivgroup.service.login.admin</groupId>
            <artifactId>login-admin</artifactId>
            <version>${login-admin.version}</version>
        </dependency>
        <dependency>
            <groupId>com.bivgroup.service.password</groupId>
            <artifactId>password-service</artifactId>
            <version>${password-service.version}</version>
        </dependency>
        <dependency>
            <groupId>com.bivgroup.flextera.insurance.bivsberfront.service</groupId>
            <artifactId>department-service</artifactId>
            <version>${department-service.version}</version>
        </dependency>
        <dependency>
            <groupId>com.bivgroup.flextera.insurance.bivsberfront.service</groupId>
            <artifactId>journal-service</artifactId>
            <version>${journal-service.version}</version>
        </dependency>
        <dependency>
            <groupId>com.bivgroup.flextera.insurance.bivsberfront.service</groupId>
            <artifactId>menu-service</artifactId>
            <version>${menu-service.version}</version>
        </dependency>
        <dependency>
            <groupId>com.bivgroup.flextera.insurance.bivsberfront.service</groupId>
            <artifactId>active-directory-service</artifactId>
            <version>1.00.00-SNAPSHOT</version>
        </dependency>
        <dependency>
            <groupId>com.bivgroup</groupId>
            <artifactId>external-caller</artifactId>
            <version>${external-caller.version}</version>
        </dependency>
        <dependency>
            <groupId>com.bivgroup.flextera.insurance.bivsberfront.utils</groupId>
            <artifactId>string-utils</artifactId>
            <version>${string-utils.version}</version>
        </dependency>
        <dependency>
            <groupId>com.bivgroup.flextera.insurance.bivsberfront.utils</groupId>
            <artifactId>config-utils</artifactId>
            <version>${config-utils.version}</version>
        </dependency>
        <dependency>
            <groupId>com.bivgroup.flextera.insurance.bivsberfront.utils</groupId>
            <artifactId>date-util</artifactId>
            <version>${date-utils.version}</version>
        </dependency>
        <dependency>
            <groupId>com.bivgroup.flextera.insurance.bivsberfront.utils</groupId>
            <artifactId>service-loader-utils</artifactId>
            <version>${service-loader-utils.version}</version>
        </dependency>
        <dependency>
            <groupId>com.bivgroup.common.audit.implementation</groupId>
            <artifactId>audit-impl</artifactId>
            <version>2.00.00</version>
        </dependency>
        <dependency>
            <groupId>com.bivgroup.flextera.insurance.bivsberfront.utils</groupId>
            <artifactId>password-utils</artifactId>
            <version>${password-utils.version}</version>
        </dependency>
        <dependency>
            <groupId>org.apache.logging.log4j</groupId>
            <artifactId>log4j-api</artifactId>
            <version>${log4j2.version}</version>
            <scope>compile</scope>
        </dependency>
        <dependency>
            <groupId>org.apache.logging.log4j</groupId>
            <artifactId>log4j-core</artifactId>
            <version>${log4j2.version}</version>
            <scope>compile</scope>
        </dependency>
        <dependency>
            <groupId>org.apache.logging.log4j</groupId>
            <artifactId>log4j-1.2-api</artifactId>
            <version>${log4j2.version}</version>
            <scope>compile</scope>
        </dependency>
        <dependency>
            <groupId>log4j</groupId>
            <artifactId>log4j</artifactId>
            <version>1.2.14</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>com.fasterxml.jackson.core</groupId>
            <artifactId>jackson-databind</artifactId>
            <version>2.5.4</version>
            <scope>provided</scope>
            <type>jar</type>
        </dependency>
        <dependency>
            <groupId>javax.validation</groupId>
            <artifactId>validation-api</artifactId>
            <version>1.1.0.Final</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>org.jboss.resteasy</groupId>
            <artifactId>resteasy-jaxrs</artifactId>
            <version>3.0.14.Final</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>org.jboss.resteasy</groupId>
            <artifactId>resteasy-validator-provider-11</artifactId>
            <version>3.0.14.Final</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>commons-beanutils</groupId>
            <artifactId>commons-beanutils</artifactId>
            <version>1.9.2</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>com.bivgroup.common.audit.interface</groupId>
            <artifactId>audit-api</artifactId>
            <version>2.00.00</version>
            <scope>compile</scope>
        </dependency>
        <dependency>
            <groupId>com.bivgroup.service.login.common</groupId>
            <artifactId>login-common</artifactId>
            <version>${login-common.version}</version>
            <scope>compile</scope>
        </dependency>
    </dependencies>

    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>2.3.2</version>
                <configuration>
                    <source>1.8</source>
                    <target>1.8</target>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-resources-plugin</artifactId>
                <version>2.2</version>
                <configuration>
                    <encoding>${project.build.sourceEncoding}</encoding>
                </configuration>
            </plugin>
        </plugins>
    </build>

    <distributionManagement>
        <repository>
            <id>biv-nexus</id>
            <name>BIV nexus repository</name>
            <url>https://192.168.1.46:8443/repository/biv-releases/</url>
        </repository>
        <snapshotRepository>
            <id>biv-nexus-snapshots</id>
            <name>BIV snapshots nexus repository</name>
            <url>https://192.168.1.46:8443/repository/biv-snapshots/</url>
        </snapshotRepository>
    </distributionManagement>
</project>
