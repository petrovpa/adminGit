<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <groupId>com.bivgroup.service.login</groupId>
    <artifactId>login</artifactId>
    <name>login</name>
    <version>1.00.00</version>
    <packaging>pom</packaging>

    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <service-caller.version>1.01.03</service-caller.version>
        <login-base.version>1.00.01</login-base.version>
        <login-common.version>1.00.01</login-common.version>
        <login-admin.version>1.00.01</login-admin.version>
        <biv-ldap.version>1.00.00-SNAPSHOT</biv-ldap.version>
        <service-utils.version>1.01.01</service-utils.version>
        <string-utils.version>1.00.02</string-utils.version>
        <xml-utils.version>1.00.00</xml-utils.version>
        <password-utils.version>1.00.01</password-utils.version>
        <service-loader-utils.version>1.00.00</service-loader-utils.version>
        <log4j2.version>2.6.2</log4j2.version>
        <session-utils.version>1.2</session-utils.version>
    </properties>

    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>2.0.2</version>
                <configuration>
                    <encoding>${project.build.sourceEncoding}</encoding>
                    <source>1.8</source>
                    <target>1.8</target>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-resources-plugin</artifactId>
                <version>2.2</version>
                <configuration>
                    <encoding>${project.build.sourceEncoding}</encoding>
                </configuration>
            </plugin>
        </plugins>
    </build>
    <dependencies>
        <dependency>
            <groupId>com.bivgroup</groupId>
            <artifactId>service-caller</artifactId>
            <version>${service-caller.version}</version>
        </dependency>
        <dependency>
            <groupId>com.bivgroup.common.audit.interface</groupId>
            <artifactId>audit-api</artifactId>
            <version>2.00.00</version>
        </dependency>
        <dependency>
            <groupId>com.bivgroup</groupId>
            <artifactId>biv-ldap</artifactId>
            <version>1.00.00-SNAPSHOT</version>
            <scope>compile</scope>
        </dependency>
        <dependency>
            <groupId>com.bivgroup.flextera.insurance.bivsberfront.utils</groupId>
            <artifactId>password-utils</artifactId>
            <version>1.00.01</version>
            <scope>compile</scope>
        </dependency>
        <dependency>
            <groupId>com.bivgroup.flextera.insurance.bivsberfront.utils</groupId>
            <artifactId>sessionutils</artifactId>
            <version>1.2</version>
            <scope>compile</scope>
        </dependency>
        <dependency>
            <groupId>com.bivgroup.flextera.insurance.bivsberfront.utils</groupId>
            <artifactId>xml-utils</artifactId>
            <version>1.00.00</version>
            <scope>compile</scope>
        </dependency>
        <dependency>
            <groupId>com.bivgroup.flextera.insurance.bivsberfront.utils</groupId>
            <artifactId>service-loader-utils</artifactId>
            <version>1.00.00</version>
            <scope>compile</scope>
        </dependency>
        <dependency>
            <groupId>com.bivgroup.flextera.insurance.bivsberfront.utils</groupId>
            <artifactId>service-utils</artifactId>
            <version>${service-utils.version}</version>
            <scope>compile</scope>
        </dependency>
    </dependencies>
    <modules>
        <module>login-base</module>
        <module>login-common</module>
        <module>login-admin</module>
    </modules>
</project>